{
  "name": "Typeflow",
  "version": "0.1.2",
  "icon": "icon.ico",
  "description": "Markdown editor with inline live preview for everyone, anywhere.",
  "main": "app/init/main.js",
  "scripts": {
    "start": "electron .",
    "build": "esbuild app/renderer/index.js --bundle --platform=node --outdir=app/esbuild | electron .",
    "packtowin": "electron-packager . Typeflow --platform=win32 --arch=x64 --overwrite --out dist/ --icon icons/icon.ico --prune=true --win32metadata.requested-execution-level=requireAdministrator",
    "packtomac": "electron-packager . Typeflow --platform=darwin --arch=x64 --overwrite --out dist/ --icon icons/icon.ico --prune=true"
  },
  "author": "L33dy",
  "license": "ISC",
  "dependencies": {
    "marked": "^4.2.5",
    "turndown": "^7.1.1"
  },
  "devDependencies": {
    "electron": "^22.0.3",
    "electron-packager": "^17.1.1",
    "esbuild": "^0.17.5"
  }
}
